      <word id="13" x="6" y="2-5"/>
      <word id="14" x="7" y="2-5"/>
      <word id="15" x="7" y="3-5"/>
      <clues>
        <title>Across</title>
        <clue word="1" number="1">See 3-Down</clue>
        <clue word="4" number="5">See 1- and 6-Across</clue>
        <clue word="5" number="6">Normal clue</clue>
        <clue word="6" number="8">See 1-, 5- and 10-Across and 3- and 4-Down</clue>
        <clue word="7" number="9">Normal clue</clue>
        <clue word="8" number="10">See 2-Down</clue>
      </clues>
      <clues>
        <title>Down</title>
        <clue word="9" number="1">9-Across, for example</clue>
        <clue word="11" number="2">Normal clue</clue>
        <clue word="13" number="3">See 4-Down</clue>
        <clue word="14" number="4">See 3-Down and 1-, 6- and 9-Across</clue>
        <clue word="15" number="7">Will be removed</clue>
      </clues>
