        <cell x="3" y="2" solution="X"/>
        <cell x="4" y="2" type="block"/>
        <cell x="5" y="2" solution="X"/>
        <cell x="6" y="2" solution="X" number="3"/>
        <cell x="7" y="2" solution="X" number="4"/>
        <cell x="1" y="3" solution="X" number="5"/>
        <cell x="2" y="3" solution="X"/>
        <cell x="3" y="3" solution="X"/>
        <cell x="4" y="3" type="block"/>
        <cell x="5" y="3" solution="X" number="6"/>
        <cell x="6" y="3" solution="X"/>
        <cell x="7" y="3" solution="X" number="7"/>
        <cell x="1" y="4" solution="X" number="8"/>
        <cell x="2" y="4" solution="X"/>
        <cell x="3" y="4" solution="X"/>
        <cell x="4" y="4" type="block"/>
        <cell x="5" y="4" solution="X" number="9"/>
        <cell x="6" y="4" solution="X"/>
        <cell x="7" y="4" solution="X"/>
        <cell x="1" y="5" type="block"/>
        <cell x="2" y="5" type="block"/>
        <cell x="3" y="5" type="block"/>
        <cell x="4" y="5" type="block"/>
        <cell x="5" y="5" solution="X" number="10"/>
        <cell x="6" y="5" solution="X"/>
        <cell x="7" y="5" solution="X"/>
      </grid>
      <word id="1" x="1-3" y="1"/>
      <word id="2" x="1-3" y="2"/>
      <word id="3" x="5-7" y="2"/>
      <word id="4" x="1-3" y="3"/>
      <word id="5" x="5-7" y="3"/>
