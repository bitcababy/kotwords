    <metadata>
      <title>Test</title>
      <creator>Test</creator>
      <copyright>Test</copyright>
      <description/>
    </metadata>
    <crossword>
      <grid width="7" height="5">
        <cell x="1" y="1" solution="X" number="1"/>
